<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="Multishipping Checkout Customer Address Edit Form" design_abstraction="custom">
    <update handle="customer_form_template_handle"/>
     <head>
        <css src="Webkul_GoogleMapPinAddress::css/style-map.css"/>
    </head>
   <body>
        <referenceBlock name="customer_address_edit">
            <action method="setTemplate" ifconfig="googlemappinaddress/gmpa_settings/active">
                <argument name="template" xsi:type="string">Webkul_GoogleMapPinAddress::edit.phtml</argument>
            </action>
            <arguments>
                    <argument name="wk_map" xsi:type="object">Webkul\GoogleMapPinAddress\Helper\MapData</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>    