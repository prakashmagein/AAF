<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="google" type="host">https://maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="google-map" type="host">https://maps.gstatic.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="google-apis" type="host">https://maps.googleapis.com</value>
            </values>
        </policy>
        <policy id ="font-src">
            <values>
                <value id="font-api" type = "host">https://fonts.gstatic.com/</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
