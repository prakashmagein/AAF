<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Lof_ProductShipping::lofproduct_shipping" title="Product Shipping" sortOrder="1">
                    <resource id="Lof_ProductShipping::createnew_shipping" title="Create New Product Shipping" sortOrder="1"/>
                    <resource id="Lof_ProductShipping::save_shipping" title="Save Product Shipping" sortOrder="2"/>
                    <resource id="Lof_ProductShipping::edit_shipping" title="Edit Product Shipping" sortOrder="3"/>
                    <resource id="Lof_ProductShipping::delete_shipping" title="Delete Product Shipping" sortOrder="4"/>
                    <resource id="Lof_ProductShipping::export_tocsv" title="Export Product Shipping To CSV File" sortOrder="5"/>
                    <resource id="Lof_ProductShipping::upload_shipping" title="Import Product Shipping From CSV File" sortOrder="6"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
