<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Catalog\Block\Product\ListProduct" name="gwl_search_result_index" template="Gwl_Datalayerevents::category/list.phtml" after="-">
                <arguments>
                    <argument name="view_model" xsi:type="object">Gwl\Datalayerevents\Block\Category\ListProduct</argument>
                </arguments>
                <referenceBlock name="search_result_list" template="Gwl_Datalayerevents::product/list.phtml">
                    <arguments>
                        <argument name="view_model_search" xsi:type="object">Gwl\Datalayerevents\Block\Product\ListProduct</argument>
                    </arguments>
                </referenceBlock>
            </block>
        </referenceContainer>
    </body>
</page>
