<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="smsprofilelog_clear" instance="Magedelight\SMSProfile\Cron\SmsProfileClearLog" method="execute">
            <config_path>crontab/default/jobs/smsprofilelog_clear/schedule/cron_expr</config_path>
        </job>

        <job name="change_smsprofile_status" instance="Magedelight\SMSProfile\Cron\ChangeSmsStatus" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <!-- SMS Notification --> 
        <job instance="Magedelight\SMSProfile\Cron\ClearLog" method="execute" name="smslog_clear">
            <config_path>crontab/default/jobs/smslog_clear/schedule/cron_expr</config_path>         
        </job>

        <job name="change_sms_status" instance="Magedelight\SMSProfile\Cron\ChangeTwilioSmsStatus" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>      
    </group>
</config>